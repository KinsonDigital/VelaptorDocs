{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
		{
            "name": "Playground (DEBUG)",
            "request": "launch",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/playground.ts",
            "cwd": "${workspaceFolder}",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all"
            ],
			"args": ["${workspaceFolder}"],
            "attachSimplePort": 9229,
            "windows": {
                "runtimeExecutable": "${userHome}/.deno/bin/deno.exe"
            },
            "linux": {
                "runtimeExecutable": "${userHome}/.deno/bin/deno"
            }
        },
        {
            "request": "launch",
            "name": "Launch (With Doc Reset)",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/scripts/generate-new-api-docs.ts",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "reset-docs",
            "runtimeExecutable": "${userHome}/.deno/bin/deno.EXE",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all",
            ],
            "args": [
                "${workspaceFolder}/versioned_docs/version-1.0.0-testing/api",
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "Launch (Without Doc Reset)",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/scripts/generate-new-api-docs.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${userHome}/.deno/bin/deno.EXE",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all",
            ],
            "args": [
                "${workspaceFolder}/versioned_docs/version-1.0.0-testing/api",
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "Reset Docs",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/manual-testing/reset-docs.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${userHome}/.deno/bin/deno.EXE",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all",
            ],
            "attachSimplePort": 9229
        },
		{
            "request": "launch",
            "name": "Delete API Docs (DEBUG)",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/scripts/delete-api-docs.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${userHome}/.deno/bin/deno.EXE",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all",
            ],
            "attachSimplePort": 9229,
			"console": "integratedTerminal"
        },
		{
            "request": "launch",
            "name": "Delete Oldest API Docs (DEBUG)",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/scripts/delete-oldest-api-docs.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${userHome}/.deno/bin/deno.EXE",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-read",
				"--allow-write",
            ],
            "attachSimplePort": 9229,
			"console": "integratedTerminal"
        },
        {
            "request": "launch",
            "name": "Enable Testing Env",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/manual-testing/testing-env.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${userHome}/.deno/bin/deno.EXE",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all",
            ],
            "args": [
                "enable",
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "Update Website Version",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/scripts/update-website-version.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${userHome}/.deno/bin/deno.EXE",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all",
            ],
            "attachSimplePort": 9229
        },
		{
            "request": "launch",
            "name": "Generate New API Docs",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/scripts/generate-new-api-docs.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${userHome}/.deno/bin/deno.EXE",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all",
            ],
			"args": [
				"${workspaceFolder}/docs/api",
				"v1.0.0-preview.23",
				"false" // Is Interactive
			],
			"console": "integratedTerminal",
            "attachSimplePort": 9229
        },
		{
            "request": "launch",
            "name": "Velaptor Tag Exists",
            "type": "node",
            "program": "${workspaceFolder}/.github/cicd/scripts/velaptor-version-exists.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${userHome}/.deno/bin/deno.EXE",
            "runtimeArgs": [
                "run",
                "--inspect-wait",
                "--allow-all",
            ],
			"args": [
				"v1.0.0-preview.12",
			],
            "attachSimplePort": 9229
        }
    ]
}
