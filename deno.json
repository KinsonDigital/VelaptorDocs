{
	"imports": {
		"std/": "https://deno.land/std@0.196.0/",
        "clients/": "https://raw.githubusercontent.com/kinsondigital/Infrastructure/v12.1.0/cicd/clients/",
        "cliffy/": "https://deno.land/x/cliffy@v1.0.0-rc.2/"
	},
	"fmt": {
        "include": [".github/cicd/.*\\.ts$"],
        "exclude": [
            ".vscode",
            ".docusaurus",
            "**/*.md",
            "versions.json",
            "tsconfig.json",
            "package.json",
            "package-lock.json",
            "deno.json",
            "default-doc-config.json",
            "sidebars.js",
            "docusaurus.config.js",
            "babel.config.js",
            ".github/cicd/.vscode/",
            ".github/cicd/manual-testing/unprocessed-docs/",
            "code_coverage/",
            "build/",
            "src/",
            "static/",
            "versioned_docs/",
            "versioned_sidebars/",
            "docs/",
            "blog/",
            "RepoSrc/"
        ],
        "useTabs": true,
        "lineWidth": 130,
        "indentWidth": 4,
        "semiColons": true,
        "singleQuote": false
	},
	"lint": {
        "include": [".github/cicd/.*\\.ts$"],
        "exclude": [
            "**/playground.ts",
            "README.md",
            "versions.json",
            "tsconfig.json",
            "package.json",
            "package-lock.json",
            "deno.json",
            "settings.json",
            "default-doc-config.json",
            "sidebars.js",
            "docusaurus.config.js",
            "babel.config.js",
            ".docusaurus/",
            "code_coverage/",
            "build/",
            "src/",
            "static/",
            "versioned_docs/",
            "versioned_sidebars/",
            "docs/",
            "blog/",
            "RepoSrc/"
        ],
		"rules": {
			"include": [
				"explicit-function-return-type"
			]
		}
	}
}
